{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "strict": true,
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "jsxImportSource": "solid-js",
    "jsx": "preserve",
    "types": ["vite/client", "@solidjs/start/env"],
    "baseUrl": "./",
    "paths": {
      "~/*": ["./src/*"],
      "~": ["./src"]
    },
    "allowImportingTsExtensions": true,
    "noEmit": true
  }
}
